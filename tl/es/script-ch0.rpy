translate es sisterhood_ch0_sh_ch0_5c774631:

    # "Are we still talking about Go here?"
    "¿Seguimos hablando de Go?"

# game/mods/sisterhood/script-ch0.rpy:258
translate es sisterhood_ch0_sh_ch0_9930b52c:

    # ha "Ah…"
    ha "Ah…"

# game/mods/sisterhood/script-ch0.rpy:261
